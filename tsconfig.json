{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext", 
    "moduleResolution": "node",
    // "lib": ["dom", "esnext"],
    "jsx": "preserve",
    "sourceMap": false,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"],
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  }
}
